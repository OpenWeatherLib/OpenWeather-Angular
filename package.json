{
  "name": "open-weather-angular",
  "version": "0.8.1",
  "description": "Angular 8 library to grab data from openweathermap.org",
  "author": {
    "name": "Jonas Schubert",
    "url": "https://github.com/OpenWeatherLib/OpenWeather-Angular",
    "email": "guepardoapps@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --runInBand",
    "lint": "eslint \"src/**/*.ts\" && sass-lint --config .sasslintrc",
    "lint-p": "eslint \"src/**/*.ts\" & sass-lint --config .sasslintrc",
    "e2e": "ng e2e",
    "documentation": "npx compodoc -p src/tsconfig.app.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.13",
    "@angular/cdk": "8.2.3",
    "@angular/common": "8.2.13",
    "@angular/compiler": "8.2.13",
    "@angular/core": "8.2.13",
    "@angular/forms": "8.2.13",
    "@angular/material": "8.2.3",
    "@angular/platform-browser": "8.2.13",
    "@angular/platform-browser-dynamic": "8.2.13",
    "@angular/router": "8.2.13",
    "@ngrx/effects": "8.4.0",
    "@ngrx/entity": "8.4.0",
    "@ngrx/store": "8.4.0",
    "@ngrx/store-devtools": "8.4.0",
    "ajv": "6.10.2",
    "bootstrap": "4.3.1",
    "core-js": "2.6.9",
    "jquery": "3.4.1",
    "hammerjs": "2.0.8",
    "popper.js": "1.16.0",
    "protractor": "5.4.2",
    "rxjs": "6.5.3",
    "rxjs-compat": "6.5.3",
    "timext-js": "0.7.1",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.16",
    "@angular/cli": "8.3.16",
    "@angular/compiler-cli": "8.2.13",
    "@angular/language-service": "8.2.13",
    "@compodoc/compodoc": "1.1.11",
    "@types/jasmine": "3.4.4",
    "@types/jasminewd2": "2.0.8",
    "@types/jest": "24.0.21",
    "@types/node": "12.12.3",
    "@typescript-eslint/eslint-plugin": "2.6.0",
    "@typescript-eslint/parser": "2.6.0",
    "eslint": "6.6.0",
    "jasmine-marbles": "0.6.0",
    "jest": "24.9.0",
    "jest-html-reporter": "2.6.2",
    "jest-preset-angular": "8.0.0",
    "node-sass": "4.13.0",
    "sass-lint": "1.13.1",
    "ts-node": "8.4.1",
    "typescript": "3.5.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ]
  }
}
